{
    "name": "prom-php-exporter/laravel-exporter",
    "description": "A short description of what your package does",
    "require": {
        "php": "^7.2",
        "promphp/prometheus_client_php": "^2.1",
        "illuminate/support": "^5.3 || ^6.0 || ^7.0",
        "illuminate/routing": "^5.3 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "PromPhpExporter\\LaravelExporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PromPhpExporter\\LaravelExporter\\PrometheusServiceProvider"
            ],
            "aliases": {
                "prometheus": "PromPhpExporter\\LaravelExporter\\PrometheusFacade"
            }
        }
    }
}
